{
  "overwrite-config": true,
  "write-logs": true,
  "one-file": false,
  "log-paths": {
    "error": "./LogTests/Logs/error",
    "info": "./LogTests/Logs/info",
    "debug": "./LogTests/Logs/debug",
    "warn": "./LogTests/Logs/warn"
  },
  "log-patterns": {
    "error": "(<time>)\t[<type>]\t<message>",
    "info": "(<time>)\t[<type>]\t<message>",
    "debug": "(<time>)\t[<type>]\t<message>",
    "warn": "(<time>)\t[<type>]\t<message>"
  },
  "log-colors": {
    "error": {
      "text": "black",
      "background": "red"
    },
    "info": {
      "text": "black",
      "background": "white"
    },
    "debug": {
      "text": "black",
      "background": "green"
    },
    "warn": {
      "text": "black",
      "background": "yellow"
    }
  },
  "custom-logs": [
    {
      "name": "jeff",
      "aliases": [],
      "path": "./LogTests/Logs/custom/jeff",
      "color": {
        "text": "red",
        "background": "white"
      },
      "pattern": "<time>\t[<type>]\t<message>"
    },
    {
      "name": "panda",
      "aliases": [],
      "path": "./LogTests/Logs/custom/panda",
      "color": {
        "text": "black",
        "background": "white"
      },
      "pattern": "<time>\t[<type>]\t'nom'\t<message>"
    }
  ],
  "time-pattern": "MMMM DD YYYY, h:mm:ss a"
}